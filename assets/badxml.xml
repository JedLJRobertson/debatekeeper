<?xml version="1.0" encoding="UTF-8"?>
<debateformat name="Test bad XML" schemaversion="1.0">
	<info>
		<region>None</region>
		<level>None</level>
		<usedat>Nowhere</usedat>
		<desc>This format shouldn't load, the XML is messed up.</desc>
	</info>

	<resource ref="#all"
		<period ref="initial" desc="First minute" bgcolor="#stay" />
		<period ref="warning" desc="Warning bell rung" bgcolor="#77ffcc00" />
		<period ref="overtime" desc="Overtime" bgcolor="#77ff0000" />
	</resource>

	<speechtype ref="all" length="8:00" firstperiod="initial">
		<bell time="6:00" number="1" nextperiod="warning" />
		<bell time="finish" number="2" nextperiod="overtime" />
	</speechtype>

	<speechtype ref="reply" length="4:00" firstperiod="initial">
		<bell time="3:00" number="1" nextperiod="warning" />
		<bell time="finish" number="2" nextperiod="overtime" />
	</speechtype>
	
	<speeches>
		<speech name="1st Affirmative" type="all" />
		<speech name="1st Negative" type="all" />
		<speech name="2nd Affirmative" type="all" />
		<speech name="2nd Negative" type="all" />
		<speech name="3rd Affirmative" type="all" />
		<speech name="3rd Negative" type="all" />
	</speeches>

</debateformat>