<!-- This file contains the global period types. -->
<period-types>
	<period-type ref="normal">
		<name>Normal</name>
	</period-type>
	<period-type ref="pois-allowed" pois-allowed="true">
		<name>POIs allowed</name>
		<default-bgcolor>#77669900</default-bgcolor>
	</period-type>
	<period-type ref="warning">
		<name>Warning</name>
		<default-bgcolor>#77ff8800</default-bgcolor>
	</period-type>
	<period-type ref="overtime">
		<name>Overtime</name>
		<default-bgcolor>#77cc0000</default-bgcolor>
	</period-type>
</period-types>